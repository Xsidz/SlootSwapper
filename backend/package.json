{
    "name": "slot-swapper-backend",
    "version": "1.0.0",
    "description": "Backend API for SlotSwapper application",
    "main": "server.js",
    "type": "module",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "test": "node --test tests/unit.test.js",
        "test:integration": "node run-tests.js",
        "test:raw": "node --test tests/*.test.js",
        "test:watch": "node --test --watch tests/*.test.js",
        "lint": "eslint . --ext js --report-unused-disable-directives --max-warnings 0",
        "lint:fix": "eslint . --ext js --fix",
        "format": "prettier --write \"**/*.{js,json,md}\""
    },
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.0.0",
        "express": "^4.18.0",
        "express-rate-limit": "^6.7.0",
        "express-validator": "^6.15.0",
        "jsonwebtoken": "^9.0.0",
        "mongoose": "^7.0.0"
    },
    "devDependencies": {
        "eslint": "^8.38.0",
        "nodemon": "^2.0.20",
        "prettier": "^2.8.0"
    }
}